{
  "functions": {
    "app/(main)/api/**/route.ts": {
      "includeFiles": "node_modules/tesseract.js-core/tesseract-core-simd.wasm",
      "memory": 1024,
      "maxDuration": 60
    }
  },  
  "crons": [
    {
      "path": "/api/(main)/cron",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/(main)/dailyCron",
      "schedule": "0 0 * * *"
    }
  ]
}